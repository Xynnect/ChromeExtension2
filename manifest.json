{
  "manifest_version": 2,

  "name": "GTmetrix Analyzer Plugin",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.0",

  "app": {
    "background": {
      "scripts": ["background.js","inject.js"]
    }
  },
  "permissions": [
    "unlimitedStorage" ,
    "activeTab" , "http://*/*", "https://*/*"
  ]
}